<div class="container">
  <div class="login-form">
    <table class="table table-bordered">
      <tr>
        <th>User ID</th>
        <th>Username</th>
        <th>Password</th>
        <th>Roles</th>
      </tr>
      <% @login.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.username %></td>
        <td><%= user.password%></td>
        <td></td>
      </tr>
      <% end %>
    </table>
  </div>
</div>